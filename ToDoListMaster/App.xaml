<Application x:Class="ToDoListMaster.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ToDoListMaster"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <local:BooleanToYesNoConverter x:Key="BooleanToYesNoConverter"/>
        <local:NullToAddConverter x:Key="NullToAddConverter"/>
        <local:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
        <local:NullToHiddenConverter x:Key="NullToHiddenConverter"/>
        <local:NullToVisibilityConverter x:Key="NullToVisibilityConverter"/>
    </Application.Resources>
</Application>